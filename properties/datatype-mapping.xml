  <alias name="Hashtable"/>
</alias-list>

<alias-list name="java.util.Set">
  <alias name="Set"/>
</alias-list>

<alias-list name="java.util.Vector">
  <alias name="Vector"/>
</alias-list>

<alias-list name="java.math.BigDecimal">
  <alias name="bigdecimal"/>
</alias-list>

<alias-list name="java.sql.Timestamp">
  <alias name="timestamp"/>
