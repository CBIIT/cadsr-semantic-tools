  <alias name="byte[]"/>
  <alias name="Byte[]"/>
  <alias name="java.lang.Byte[]"/>
</alias-list>

<alias-list name="anyClass Three Dimensional Array">
  <alias name="object[][][]"/>
  <alias name="Object[][][]"/>
  <alias name="java.lang.Object[][][]"/>
</alias-list>

<alias-list name="java.lang.IntegerArray">
  <alias name="int[]"/>
  <alias name="Integer[]"/>
  <alias name="java.lang.Integer[]"/>
</alias-list>

<alias-list name="java.lang.Object">
  <alias name="Object"/>
</alias-list>

<alias-list name="java.lang.StringArrayArray">
  <alias name="String[][]"/>
  <alias name="java.lang.String[][]"/>
</alias-list>

<alias-list name="java.lang.Void">
  <alias name="void"/>
